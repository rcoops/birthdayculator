{
  "name": "birthdayculator",
  "version": "0.0.1",
  "description": "A set of utility functions for calculating age, duration until next birthday, age at and date of next birthday etc.",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && node build.js",
    "watch": "rimraf dist && node build.js --dev",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "precommit": "npm run test && npm run lint && prettier . -c",
    "format": "npm run lint -- --fix && prettier . -w",
    "prepare": "husky install"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "age",
    "birthday",
    "birth-date"
  ],
  "author": "Rick Cooper <rcoops84@hotmail.com>",
  "repository": {
    "url": "https://github.com/rcoops/birthdayculator"
  },
  "license": "ISC",
  "dependencies": {
    "luxon": "^2.4.0"
  },
  "peerDependencies": {
    "luxon": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^27.5.1",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "esbuild": "^0.14.39",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "npm-dts": "^1.3.11",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.6.4"
  }
}
